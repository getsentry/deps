itHub's blog: [From 48k lines of code to 10—the story of GitHub’s JavaScript SDK](https://github.blog/2020-04-09-from-48k-lines-of-code-to-10-the-story-of-githubs-javascript-sdk/)\n\n## LICENSE\n\n[MIT](LICENSE)"},"npm":{"downloads":[{"from":"2021-08-30T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":628962},{"from":"2021-08-24T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":3014930},{"from":"2021-08-01T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":14566910},{"from":"2021-06-02T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":44373006},{"from":"2021-03-04T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":85843407},{"from":"2020-08-31T00:00:00.000Z","to":"2021-08-31T00:00:00.000Z","count":160262756}],"dependentsCount":1273,"starsCount":3},"github":{"starsCount":97,"forksCount":10,"subscribersCount":7,"issues":{"count":102,"openCount":18,"distribution":{"3600":24,"10800":4,"32400":5,"97200":20,"291600":7,"874800":0,"2624400":2,"7873200":6,"23619600":12,"70858800":13,"212576400":9},"isDisabled":false},"contributors":[{"username":"felicitymay","commitsCount":1},{"username":"zenorocha","commitsCount":3},{"username":"seidtgeist","commitsCount":2},{"username":"kuba-kubula","commitsCount":3},{"username":"srawlins","commitsCount":1},{"username":"Krinkle","commitsCount":2},{"username":"adamlofting","commitsCount":2},{"username":"zefhemel","commitsCount":2},{"username":"greenkeeper[bot]","commitsCount":125},{"username":"octokitbot","commitsCount":4},{"username":"Ethan-Arrowood","commitsCount":2},{"username":"billytrend","commitsCount":2},{"username":"williamkapke","commitsCount":3},{"username":"oscard0m","commitsCount":1},{"username":"orta","commitsCount":5},{"username":"boblauer","commitsCount":3},{"username":"MaximDevoir","commitsCount":1},{"username":"kaizensoze","commitsCount":41},{"username":"jamestalmage","commitsCount":2},{"username":"codydaig","commitsCount":2},{"username":"greggman","commitsCount":4},{"username":"roonyh","commitsCount